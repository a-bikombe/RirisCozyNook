{
    "buildCommand": "ng build --configuration production",
    "outputDirectory": "dist/riris-cozy-nook/browser",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "functions": {
        "api/**/*.ts": {
            "runtime": "edge"
        },
        "api/**/*.js": {
            "runtime": "edge"
        }
    }
}