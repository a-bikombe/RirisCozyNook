generator client {
  provider = "prisma-client-js"
  output   = "../api/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model crew {
  name         String
  id           Int     @id(map: "Crew_pkey") @default(autoincrement())
  relationship String?
  description  String?
  image        String?
}

model users {
  id              Int    @id @default(autoincrement())
  username        String @unique
  password        String
  display_name    String
  favorite_animal String
  neon_user_id    String @unique @db.Uuid
}

model bunny_facts {
  id   Int    @id @default(autoincrement())
  fact String
}
